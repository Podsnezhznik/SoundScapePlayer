<Page x:Class="Test1_Base.ExtractingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Test1_Base.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ExtractingPage">

    <Grid Style="{StaticResource grid}">

        <StackPanel Orientation="Vertical" Margin="30" HorizontalAlignment="Left">

            <Button x:Name="extractFromVideo" Width="200" Content="Извлечь из видео" Margin="10" Click="extractFromVideo_Click"/>

        </StackPanel>
        
    </Grid>
</Page>
